<script>
 function adjustHeaderHeight() {
   const contentDiv = document.getElementById('top-nav-content');
   const bgDiv = document.getElementById('top-nav-background');
   const topNavLinksDiv = document.getElementById('top-nav-links');
   const headerHeight = contentDiv.clientHeight;
   const linksHeight = topNavLinksDiv.clientHeight;
   bgDiv.style.height = headerHeight + linksHeight + "px";
   const automataContainer = document.getElementById("background-content");
   if (automataContainer) {
     automataContainer.innerHTML = window.Automata.generateAutomata(headerHeight + linksHeight);
   }
 }

 const resizeListener = window.addEventListener('resize', adjustHeaderHeight);
 adjustHeaderHeight()
</script>
